{{#in-element (query-selector "#application-navigation")}}
  <nav block:scope ...attributes>
    <ul block:class="list">
      {{#each @links as |link|}}
        <li block:class="item">
          <LinkTo @route={{link}}>{{t (concat "navigation." link)}}</LinkTo>
        </li>
      {{/each}}
    </ul>
  </nav>
{{/in-element}}
