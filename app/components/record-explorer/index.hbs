<Ui::Window ...attributes>
  <:title>
    {{this.headline}}
  </:title>

  <:toolbar>
    <LinkTo @route={{this.newPath}}>New</LinkTo><br><br>
  </:toolbar>

  <:body>
    <Ui::List
      @records={{@records}}
      @metaFields={{@recordFields}}
      @onClick={{this.openRecord}}
    />
  </:body>

  <:bottombar>
    Pagination
  </:bottombar>
</Ui::Window>
